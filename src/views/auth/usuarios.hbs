<div class="container p-4">
  <div class="row">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Nombre</th>
          <th scope="col">Correo</th>
          <th scope="col">Role</th>
        </tr>
      </thead>
      <tbody>
        {{#each usuarios}}
        <tr>
          <th scope="row">{{id_user}}</th>
          <td>{{nombre_user}}</td>
          <td>{{email_user}}</td>
          <td>{{role_user}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
